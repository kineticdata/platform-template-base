{
  "authStrategy": {
    "type": "Signature",
    "properties": [
      {
        "name": "Key",
        "value": "kinops-request-ce"
      },
      {
        "name": "Secret",
        "sensitive": "true"
      }
    ]
  },
  "event": "Created",
  "filter": "user('invitedBy') === null",
  "name": "User Created",
  "type": "User",
  "url": "${space('attribute:Task Server Scheme')}://${space('attribute:Task Server Host')}/kinetic-task/app/api/v1/run-tree/${space('attribute:Task Source Name')}/Users/Created",
  "attributes": null
}